<div class="booyah-box col-10 offset-1">
    <h1>Today's Goals</h1>
    <%= simple_form_for @dailygoal, url: dailygoals_path do |f| %>
        <%= f.input :title %>
        <%= f.input :description %>
        <%= f.input :cost %>
        <br />
        <%= f.submit "Enter Today's Goals", class: 'btn btn-primary' %>
        <br />
    <% end %>
